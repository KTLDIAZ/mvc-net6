@model List<MVC_CodeFirst.Domain.User>

<table class="table table-dark">
    <thead>
    <tr>
        <th>Nombre completo</th>
        <th>Email</th>
        <th>Número de telefono</th>
        <th>Estado civil</th>
        <th>Direccion</th>
        <th>Acciones</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Fullname</td>
            <td>@user.Email</td>
            <td>@user.PhoneNumber</td>
            <td>@user.Address</td>
            <td>@user.CivilStatus</td>
            <td>
                <a class="btn btn-primary" asp-area="" asp-controller="User" asp-action="Edit" asp-route-userId="@user.UserId">Editar</a>
                <a class="btn btn-danger" asp-controller="User" asp-action="Delete" asp-route-userId="@user.UserId">Eliminar</a>
            </td>
        </tr>
    }
    </tbody>
</table>